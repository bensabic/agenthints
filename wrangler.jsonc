{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "agenthints",
  "main": "src/index.ts",
  "workers_dev": false,
  "preview_urls": false,
  "compatibility_date": "2025-09-06",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true
  },
  "placement": { "mode": "smart" },
  "assets": {
    "directory": "./public/",
    "binding": "ASSETS",
    "html_handling": "drop-trailing-slash"
  },
  "routes": [
    {
      "pattern": "agenthints.tech",
      "custom_domain": true
    }
  ]
}
